@{
}


@model IEnumerable<CodeFirst.Models.Student>


<h1>This is Student Page</h1>
<a asp-action="Create">Create</a>
<ul>
    @foreach (Student stu in Model)
    {
        <li>@stu.Name, @stu.Email, @stu.Phone <a asp-action="Edit" asp-route-id="@stu.Id">Edit</a> <a asp-action="Delete" asp-route-id="@stu.Id">Delete</a></li>
    }
</ul>

